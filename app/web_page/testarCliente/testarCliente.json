    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.fds && $data.fds_cliente && $data.fds_cliente_gestor ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.fds.cliente = $data.fds_cliente;",
        "\t$data.fds.cliente.gestor = $data.fds_cliente_gestor;",
        "\treturn {",
        "\t\t//map fds variable to expected task contract input",
        "\t\tfdsInput: {",
        "\t\t\tcliente: $data.fds.cliente ? {",
        "\t\t\t\tpersistenceId_string: $data.fds.cliente.persistenceId_string !== undefined ? $data.fds.cliente.persistenceId_string : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },

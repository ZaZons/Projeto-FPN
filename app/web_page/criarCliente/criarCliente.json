      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.cliente && $data.cliente_gestor && $data.cliente_gestor_unidade_negocio ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.cliente.gestor = $data.cliente_gestor;",
        "\t$data.cliente.gestor.unidade_negocio = $data.cliente_gestor_unidade_negocio;",
        "\treturn {",
        "\t\t//map cliente variable to expected task contract input",
        "\t\tclienteInput: {",
        "\t\t\tnome: $data.cliente.nome !== undefined ? $data.cliente.nome : null,",
        "\t\t\tgestor: $data.cliente.gestor ? {",
        "\t\t\t\tpersistenceId_string: $data.cliente.gestor.persistenceId_string !== undefined ? $data.cliente.gestor.persistenceId_string : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },

    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.oportunidade_venda && $data.oportunidade_venda_cliente && $data.oportunidade_venda_cliente_gestor && $data.oportunidade_venda_cliente_gestor_unidade_negocio ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.oportunidade_venda.cliente = $data.oportunidade_venda_cliente;",
        "\t$data.oportunidade_venda.cliente.gestor = $data.oportunidade_venda_cliente_gestor;",
        "\t$data.oportunidade_venda.cliente.gestor.unidade_negocio = $data.oportunidade_venda_cliente_gestor_unidade_negocio;",
        "\treturn {",
        "\t\t//map oportunidade_venda variable to expected task contract input",
        "\t\toportunidade_vendaInput: {",
        "\t\t\tcliente: $data.oportunidade_venda.cliente ? {",
        "\t\t\t\tpersistenceId_string: $data.oportunidade_venda.cliente.persistenceId_string !== undefined ? $data.oportunidade_venda.cliente.persistenceId_string : null,",
        "\t\t\t\tgestor: $data.oportunidade_venda.cliente.gestor ? {",
        "\t\t\t\t\tpersistenceId_string: $data.oportunidade_venda.cliente.gestor.persistenceId_string !== undefined ? $data.oportunidade_venda.cliente.gestor.persistenceId_string : null,",
        "\t\t\t\t\tname: $data.oportunidade_venda.cliente.gestor.name !== undefined ? $data.oportunidade_venda.cliente.gestor.name : null,",
        "\t\t\t\t\tunidade_negocio: $data.oportunidade_venda.cliente.gestor.unidade_negocio ? {",
        "\t\t\t\t\t\tpersistenceId_string: $data.oportunidade_venda.cliente.gestor.unidade_negocio.persistenceId_string !== undefined ? $data.oportunidade_venda.cliente.gestor.unidade_negocio.persistenceId_string : null,",
        "\t\t\t\t\t\tnome: $data.oportunidade_venda.cliente.gestor.unidade_negocio.nome !== undefined ? $data.oportunidade_venda.cliente.gestor.unidade_negocio.nome : null",
        "\t\t\t\t\t} : null",
        "\t\t\t\t} : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
